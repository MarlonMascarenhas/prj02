","C:\\Users\\Marlon\\prj02\\src\\view\\home\\index.js",["68"],"import React, { useEffect, useState } from 'react';\r\nimport {useSelector} from 'react-redux';\r\n\r\nimport './styles.css';\r\nimport NavBar from '../../components/navbar';\r\nimport EventoCard from '../../components/card';\r\nimport firebase from '../../config/firebase';\r\nimport 'firebase/auth';\r\n\r\nfunction Home({match}){\r\n\r\n    const [eventos, setEventos] = useState([]);\r\n    const listaEventos = [];\r\n    const [pesquisa, setPesquisa] = useState('');\r\n    const usuarioLogado = useSelector(state => state.user.usuarioEmail);\r\n    \r\n    useEffect(() => {\r\n        if (match.params.parametro) {\r\n            firebase.firestore().collection('eventos').where('usuario', '==', usuarioLogado ).get()\r\n                .then( resultado => {\r\n                    resultado.docs.forEach(doc => {\r\n                        if(doc.data().titulo.indexOf(pesquisa) >= 0 ){\r\n                            listaEventos.push({id: doc.id, ...doc.data()})\r\n                        }\r\n                        \r\n                    })\r\n                    setEventos(listaEventos);\r\n                })\r\n            }\r\n        else {\r\n            firebase.firestore().collection('eventos').get()\r\n                .then( resultado => {\r\n                    resultado.docs.forEach(doc => {\r\n                        if(doc.data().titulo.indexOf(pesquisa) >= 0 ){\r\n                            listaEventos.push({id: doc.id, ...doc.data()})\r\n                        }\r\n                        \r\n                    })\r\n                    setEventos(listaEventos);\r\n                });\r\n        }\r\n    }, [pesquisa]);\r\n        \r\n    return (\r\n        <div>\r\n            <NavBar/>\r\n            <div className=\"row p-3\">\r\n                <h2>Eventos Publicados</h2>\r\n                <input type=\"text\" onChange={(e)=> setPesquisa(e.target.value)}\r\n                    placeholder=\"Pesquisar evento pelo titulo...\"\r\n                    className=\"form-control text-left\" />\r\n            </div>\r\n            <div className=\"row p-3\">\r\n                \r\n                { eventos.map(item => <EventoCard key={item.id} \r\n                                                img={item.foto} \r\n                                                titulo={item.titulo} \r\n                                                detalhes={item.detalhe} \r\n                                                visualizacoes={item.visualizacoes} \r\n                                                id={item.id} />) }\r\n            </div>\r\n            \r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Home;","C:\\Users\\Marlon\\prj02\\src\\components\\navbar\\index.js",[],["69","70"],"C:\\Users\\Marlon\\prj02\\src\\store.js",[],"C:\\Users\\Marlon\\prj02\\src\\reducers\\index.js",[],"C:\\Users\\Marlon\\prj02\\src\\reducers\\usuarioReducer.js",[],"C:\\Users\\Marlon\\prj02\\src\\view\\usuario-recuperaSenha\\index.js",[],"C:\\Users\\Marlon\\prj02\\src\\view\\evento-cadastro\\index.js",["71","72"],"import React, {useState, useEffect} from 'react';\r\nimport './evento-cadastro.css';\r\nimport {Redirect} from 'react-router-dom'\r\n\r\nimport { useSelector, useDispatch } from 'react-redux';\r\nimport NavBar from '../../components/navbar';\r\nimport firebase from '../../config/firebase';\r\n\r\nfunction EventoCadastro({match}){\r\n    const usuarioLogado = useSelector(state => state.user.usuarioEmail);\r\n    const [msg, setMsg] = useState(\"\");\r\n\r\n    const [titulo, setTitulo] = useState(\"\");\r\n    const [tipo, setTipo] = useState(\"\");\r\n    const [detalhe, setDetalhe] = useState(\"\");\r\n    const [data, setData] = useState(\"\");\r\n    const [hora, setHora] = useState(\"\");\r\n    const [fotoNova, setFotoNova] = useState();\r\n    const [carregando, setCarregando] = useState(0);\r\n    const [fotoAtual, setFotoAtual] = useState(0);\r\n    const [visualizacoes, setVisualizacoes] = useState(0);\r\n    const [criacao, setCriacao] = useState(new Date());\r\n\r\n    const storage = firebase.storage();\r\n    const firestore = firebase.firestore();\r\n\r\n    useEffect(()=>{\r\n        if (match.params.id){\r\n            firestore.collection('eventos').doc(match.params.id).get()\r\n                .then(resultado => {\r\n                    setTipo(resultado.data().tipo);\r\n                    setTitulo(resultado.data().titulo);\r\n                    setDetalhe(resultado.data().detalhe);\r\n                    setHora(resultado.data().hora);\r\n                    setData(resultado.data().data);\r\n                    setFotoAtual(resultado.data().foto);\r\n                    setVisualizacoes(resultado.data().visualizacoes);\r\n                    setCriacao(resultado.data().criacao);\r\n                })\r\n        }\r\n    }, []);\r\n\r\n    function atualizar(){\r\n        \r\n        setCarregando(1);\r\n        if (fotoNova)\r\n            storage.ref(`imagens/${fotoNova.name}`).put(fotoNova);\r\n\r\n            firestore.collection('eventos').doc(match.params.id).update({\r\n                titulo: titulo,\r\n                tipo: tipo,\r\n                detalhe: detalhe,\r\n                data: data,\r\n                hora: hora,\r\n                visualizacoes: visualizacoes,\r\n                publico: 1,\r\n                foto: fotoNova ? fotoNova.name : fotoAtual,\r\n                usuario: usuarioLogado,\r\n                criacao: criacao\r\n            }).then(() => { \r\n                setMsg(\"sucesso\");\r\n                setCarregando(0)\r\n                \r\n            }).catch(() =>{ \r\n                setMsg(\"erro\");\r\n                setCarregando(0) \r\n            });\r\n\r\n    }\r\n\r\n    function cadastrar(){\r\n        setCarregando(1);\r\n        \r\n        storage.ref(`imagens/${fotoNova.name + new Date()}`)\r\n                .put(fotoNova)\r\n                .then(() => {\r\n                    firestore.collection('eventos').add({\r\n                        titulo: titulo,\r\n                        tipo: tipo,\r\n                        detalhe: detalhe,\r\n                        data: data,\r\n                        hora: hora,\r\n                        visualizacoes: 0,\r\n                        publico: 1,\r\n                        foto: fotoNova.name + new Date(),\r\n                        usuario: usuarioLogado,\r\n                        criacao: new Date()\r\n                    }).then(() => { \r\n                        setMsg(\"sucesso\");\r\n                        setCarregando(0)\r\n                    }).catch(() =>{ \r\n                        setMsg(\"erro\");\r\n                        setCarregando(0) \r\n                    });\r\n\r\n                }).catch( () =>{\r\n                    setMsg(\"erro\");\r\n                    setCarregando(0) \r\n                });    \r\n    }\r\n\r\n    return (\r\n        <>\r\n            {msg === \"sucesso\" ? <Redirect to=\"/\"/> : null}\r\n            <NavBar/>\r\n\r\n            <div className=\"col-12 mt-5\">\r\n\r\n                <div className=\"row\">\r\n                    <h3 className=\"mx-auto font-weight-bold\"> {match.params.id ? 'Editar evento' : 'Novo evento'} </h3>\r\n                </div>\r\n\r\n                <form>\r\n                    <div className=\"form-group\">\r\n                        <label>Título:</label>\r\n                        <input type=\"text\" className=\"form-control\" value={titulo} onChange={(e) => setTitulo(e.target.value)}/>\r\n                    </div>\r\n\r\n                    <div className=\"form-group\">\r\n                        <label>Tipo do evento:</label>\r\n                        <select className=\"form-control\" value={tipo} onChange={(e) => setTipo(e.target.value)} >\r\n                            <option disabled selected>Selecione um tipo</option>\r\n                            <option>Festa</option>\r\n                            <option>Palestra</option>\r\n                            <option>Teatro</option>\r\n                            <option>Show</option>\r\n                        </select>\r\n                    </div>\r\n                    \r\n                    <div className=\"form-group\">\r\n                        <label>Descrição:</label>\r\n                        <textarea rows=\"3\" className=\"form-control\" value={detalhe} onChange={(e) => setDetalhe(e.target.value)}/>\r\n                    </div>\r\n\r\n                    <div className=\"form-group row\">\r\n                        <div className=\"col-6\"> \r\n                            <label>Data:</label>\r\n                            <input type=\"date\" className=\"form-control\" value={data} onChange={(e) => setData(e.target.value)} />\r\n                        </div>\r\n\r\n                        <div className=\"col-6\"> \r\n                            <label>Hora:</label>\r\n                            <input type=\"time\" className=\"form-control\" value={hora} onChange={(e) => setHora(e.target.value)}/>\r\n                        </div>\r\n                    </div>\r\n\r\n                    <div className=\"form-group\">\r\n                        <label>Upload de foto: {match.params.id ? 'caso queira manter a mesma foto, nao precisa escolher novamente' : '' }</label>\r\n                        <input type=\"file\" className=\"form-control\" onChange={(e) => setFotoNova(e.target.files[0])}/>\r\n                    </div>\r\n\r\n\r\n                    <div className=\"row\">\r\n                        {carregando === 1 ? \r\n                            <div className=\" col-3 spinner-border text-primary\" role=\"status\">\r\n                                <span className=\"visually-hidden\">Carregando...</span>\r\n                            </div>\r\n                        :\r\n                            <div className=\"col-3\"> \r\n          